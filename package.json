{
  "name": "viewintothegrowhaus",
  "version": "0.1.0",
  "engines": {
    "node": "8.9.0"
  },
  "private": true,
  "scripts": {
    "heroku-postbuild": "cd client && npm i && npm run build",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon server.js\" \"cd client && yarn start\"",
    "seed": "node seed.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^3.6.1",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "express-fileupload": "^0.4.0",
    "http-errors": "^1.6.3",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongoose": "^5.2.4",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-google-token": "^0.1.2",
    "pug": "2.0.0-beta11",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-date-picker": "^6.11.2",
    "react-dom": "^16.4.1",
    "react-google-login": "^3.2.1",
    "react-router-dom": "^4.3.1",
    "request": "^2.87.0"
  }
}
